<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:padding="20dp"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="343dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_20pxh"
        android:layout_marginTop="@dimen/_7pxv"
        android:layout_marginEnd="@dimen/_11pxh"
        android:gravity="start"
        android:orientation="horizontal"
        tools:ignore="MissingConstraints">

        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="8dp"
            app:cardCornerRadius="8dp"
            style="@style/groupStylegray_1"
            tools:ignore="MissingConstraints">
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="177dp"
                android:alpha="0.5"
                android:scaleType="centerCrop"
                android:src="@drawable/vertjaun" />
            <LinearLayout
                android:id="@+id/linearRowdeveloppeurweb"
                android:layout_width="283dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_marginEnd="@dimen/_11pxh"
                android:gravity="start"
                android:orientation="horizontal">

                <LinearLayout
                    android:id="@+id/eveloppeurweb2"
                    android:layout_width="180dp"
                    android:layout_height="150dp"
                    android:gravity="start"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/txteprog"
                        style="@style/txtInterregular10"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_5pxv"
                        android:layout_weight="0.49"
                        android:ellipsize="start"
                        android:singleLine="true"
                        android:text="AcceptÃ©"
                        android:textSize="12dp"
                        android:textColor="@color/vert" />

                    <TextView
                        android:id="@+id/txtDeveloppeurWeb"
                        style="@style/txtInterbold15"
                        android:layout_width="280dp"
                        android:layout_height="30dp"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="Developpeur Web"
                        android:textSize="18sp" />

                    <TextView
                        android:id="@+id/txtentreprise"
                        style="@style/txtIntermedium12"
                        android:layout_width="280dp"
                        android:layout_height="wrap_content"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="CGI"
                        android:textSize="18sp" />

                    <LinearLayout
                        android:id="@+id/linearRowoppeurweb"
                        android:layout_width="201dp"
                        android:layout_height="wrap_content"
                        android:gravity="start"
                        android:orientation="vertical">


                        <LinearLayout
                            android:id="@+id/linearRoweurone"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_11pxh"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_marginEnd="@dimen/_11pxh"
                            android:gravity="start"
                            android:orientation="horizontal">




                            <TextView
                                android:id="@+id/txtregion"
                                style="@style/txtIntermedium12"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_3pxh"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:ellipsize="end"
                                android:singleLine="true"
                                android:text="Paris"
                                android:textSize="14sp" />


                        </LinearLayout>


                        <LinearLayout
                            android:id="@+id/linearRowrevenusone"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_11pxh"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_marginEnd="@dimen/_11pxh"
                            android:gravity="start"
                            android:orientation="horizontal">

                            <ImageView
                                android:id="@+id/imageRevenusOne"
                                android:layout_width="@dimen/_16pxh"
                                android:layout_height="@dimen/_15pxh"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_revenus1" />

                            <TextView
                                android:id="@+id/txtPrice"
                                style="@style/txtIntermedium12"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_3pxh"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:ellipsize="end"
                                android:singleLine="true"
                                android:text="2500"
                                android:textSize="14sp" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="210dp"
                            android:layout_height="20dp"
                            android:gravity="start"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/txteDate"
                                style="@style/txtInterregular10"
                                android:layout_width="137dp"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_5pxv"
                                android:layout_weight="0.49"
                                android:ellipsize="end"
                                android:singleLine="true"
                                android:text="12_04_2024" />


                        </LinearLayout>

                    </LinearLayout>

                </LinearLayout>


                <LinearLayout
                    android:id="@+id/eveloppeurweb1"
                    android:layout_width="90dp"
                    android:layout_height="130dp"
                    android:gravity="start"
                    android:orientation="vertical">


                    <LinearLayout
                        android:id="@+id/linearRowrusone"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="bottom"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/imagupdate"
                            android:layout_width="26dp"
                            android:layout_height="29dp"
                            android:layout_marginStart="@dimen/_53pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:scaleType="fitXY"
                            android:src="@drawable/contact" />
                    </LinearLayout>

                </LinearLayout>


            </LinearLayout>
        </androidx.cardview.widget.CardView>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>